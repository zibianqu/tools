				&lt;p style=&quot;color:#373737;font-size:14px;font-family:&amp;quot;&quot;&gt;
					select&amp;nbsp; id,title FROM post WHERE MATCH(content) AGAINST (\'search keyword\' IN NATURAL LANGUAGE MODE)
